{
  "name": "text-sass",
  "version": "1.0.0",
  "description": "",
  "main": "main.scss",
  "scripts": {
    "sass": "sass --watch ./sass/main.scss:./public/css/style.css",
    "sass2": "sass --watch ./sass/palette.scss:./public/css/palette.css",
    "sass3": "sass --watch ./sass/delice.scss:./public/css/delice.css",
    "sass4": "sass --watch ./sass/francaise.scss:./public/css/francaise.css",
    "sass5": "sass --watch ./sass/enchantee.scss:./public/css/enchantee.css",
    "prefix": "postcss ./public/css/style.css --use autoprefixer -d ./public/css/prefixed/style.css",
    "prefix2": "postcss ./public/css/palette.css --use autoprefixer -d ./public/css/prefixed/palette.css",
    "prefix3": "postcss ./public/css/delice.css --use autoprefixer -d ./public/css/prefixed/delice.css",
    "prefix4": "postcss ./public/css/francaise.css --use autoprefixer -d ./public/css/prefixed/francaise.css",
    "prefix5": "postcss ./public/css/enchantee.css --use autoprefixer -d ./public/css/prefixed/enchantee.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Priime31/Projet3.git"
  },
